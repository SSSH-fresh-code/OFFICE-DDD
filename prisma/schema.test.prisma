datasource db {
  provider = "sqlite"
  url      = "file:./dev.db?mode=memory&cache=shared"
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id        String   @id @default(uuid())
  email     String   @unique
  password  String
  name      String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
